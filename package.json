{
  "name": "ima-adpcm-decoder",
  "version": "1.0.0",
  "description": "Decode IMA ADPCM WAV Files to AudioContext",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --declaration",
    "asc:production": "asc assembly/index.ts --target release -O3 --runtime minimal --exportRuntime",
    "asc:build": "asc assembly/index.ts --target debug --debug --runtime minimal --exportRuntime --sourceMap http://localhost:1234/untouched.wasm.map",
    "asc:watch": "npm run asc:production && chokidar 'assembly/*.ts' -c 'npm run asc:production && echo done'",
    "rs:dev": "wasm-pack build --target web rust",
    "rs:watch": "npm run rs:dev && chokidar 'rust/**/*.rs' -c 'npm run rs:dev && echo done'",
    "browser": "parcel src/test/index.html"
  },
  "keywords": [
    "ima",
    "adpcm",
    "audio",
    "audiocontext",
    "decoder"
  ],
  "author": "Sebastian Langer <sl@scn.cx>",
  "license": "MIT",
  "devDependencies": {
    "assemblyscript": "^0.19.20",
    "chokidar-cli": "^2.1.0",
    "parcel": "^2.0.1",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.19.20",
    "binary-parser": "^2.0.2",
    "regenerator-runtime": "^0.13.7",
    "wavefile": "^11.0.0"
  }
}
